model hero_wod {
    id          Int      @id @default(autoincrement())
    title       String   @unique
    description String   @db.Text
    history     String?
    created_at  DateTime @default(now())
}
datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}
model users {
    id            Int       @id @default(autoincrement())
    user_uuid     String    @unique
    name          String?
    email         String    @unique
    password      String?
    google_id     String?
    refresh_token String?
    created_at    DateTime  @default(now())
    updated_at    DateTime?
    archived_at   DateTime?
}
