model hero_wod {
    id          Int      @id @default(autoincrement())
    title       String   @unique
    description String   @db.Text
    history     String?
    created_at  DateTime @default(now())
}
generator client {
    provider = "prisma-client-js"
}

//데이터 베이스 연동
datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}
